  <script>
    const rarityTypes = ["Common", "Uncommon", "Rare", "Epic", "Legendary"];
    let artCount = 1; // ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏µ‡πà 1 ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏™‡∏£‡πâ‡∏≤‡∏á ID #0001 (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö NFT ‡∏ä‡∏¥‡πâ‡∏ô‡πÄ‡∏î‡∏µ‡∏¢‡∏ß)
    
    /**
     * ‡∏î‡∏∂‡∏á‡∏Ñ‡πà‡∏≤ 'token_id' ‡∏à‡∏≤‡∏Å URL Parameters (‡∏ã‡∏∂‡πà‡∏á OpenSea/‡∏£‡∏∞‡∏ö‡∏ö‡∏à‡∏∞‡∏™‡πà‡∏á‡∏Ñ‡πà‡∏≤‡∏°‡∏≤‡πÉ‡∏´‡πâ)
     * ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÅ‡∏ï‡πà‡∏•‡∏∞ NFT ‡∏°‡∏µ ID ‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏ã‡πâ‡∏≥‡∏Å‡∏±‡∏ô‡∏ï‡∏≤‡∏°‡πÇ‡∏ó‡πÄ‡∏Ñ‡∏ô
     */
    function getTokenId() {
      const urlParams = new URLSearchParams(window.location.search);
      // OpenSea ‡∏ö‡∏≤‡∏á‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏™‡πà‡∏á 'token_id' ‡∏´‡∏£‡∏∑‡∏≠ 'id'
      let id = urlParams.get('token_id') || urlParams.get('id'); 

      // ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÉ‡∏ô URL ‡πÉ‡∏´‡πâ‡∏ï‡∏±‡πâ‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô (‡πÄ‡∏ä‡πà‡∏ô 1) 
      // ‡∏´‡∏£‡∏∑‡∏≠‡πÉ‡∏ä‡πâ Math.random() ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏á‡∏≤‡∏ô Generative
      if (!id) {
          // ‡∏´‡∏≤‡∏Å‡πÑ‡∏°‡πà‡∏°‡∏µ token_id, ‡πÉ‡∏ä‡πâ‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏á‡∏ó‡∏µ‡πà‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ï‡πà‡∏•‡∏∞ NFT 
          // ‡∏´‡∏£‡∏∑‡∏≠‡πÉ‡∏ä‡πâ‡∏Å‡∏≤‡∏£‡∏™‡∏∏‡πà‡∏°‡∏ñ‡πâ‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏á‡∏≤‡∏ô Generative ‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏ã‡πâ‡∏≥‡∏Å‡∏±‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä
          return '1'; 
      }
      return id;
    }

    /**
     * ‡∏™‡∏£‡πâ‡∏≤‡∏á Seed ‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏≠‡∏Å‡∏•‡∏±‡∏Å‡∏©‡∏ì‡πå‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Generative Art 
     * ‡∏à‡∏≤‡∏Å Token ID ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏á‡∏≤‡∏ô‡∏®‡∏¥‡∏•‡∏õ‡∏∞‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏ä‡∏¥‡πâ‡∏ô‡πÑ‡∏°‡πà‡∏ã‡πâ‡∏≥‡∏Å‡∏±‡∏ô‡πÅ‡∏•‡∏∞‡∏Ñ‡∏á‡∏ó‡∏µ‡πà 
     */
    function getSeed(tokenId) {
        // Simple hashing (not cryptographically secure, but good enough for generative art seed)
        let hash = 0;
        for (let i = 0; i < tokenId.length; i++) {
            hash = tokenId.charCodeAt(i) + ((hash << 5) - hash);
        }
        return hash;
    }
    
    // Simple PRNG (Pseudo-Random Number Generator) based on a seed
    let seed = getSeed(getTokenId());
    function seededRandom() {
        // Lehmer random number generator (Park-Miller)
        seed = (seed * 9301 + 49297) % 233280;
        return seed / 233280;
    }

    // ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Math.random() ‡πÄ‡∏î‡∏¥‡∏°‡∏à‡∏∞‡∏ñ‡∏π‡∏Å‡πÅ‡∏ó‡∏ô‡∏ó‡∏µ‡πà‡∏î‡πâ‡∏ß‡∏¢ seededRandom() 
    // ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏Å‡∏≤‡∏£‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏á‡∏≤‡∏ô‡∏®‡∏¥‡∏•‡∏õ‡∏∞‡∏Ñ‡∏á‡∏ó‡∏µ‡πà‡∏ï‡∏≤‡∏° Token ID
    const _originalMathRandom = Math.random;
    Math.random = seededRandom; 
    
    function generateSkeletonPixelArt(canvas, glitchIntensity = 0.3) {
      const ctx = canvas.getContext('2d');
      const width = canvas.width;
      const height = canvas.height;
      const pixelSize = 8;
      const gridWidth = width / pixelSize;
      const gridHeight = height / pixelSize;

      // Clear canvas
      ctx.fillStyle = '#000000';
      ctx.fillRect(0, 0, width, height);

      // Skeleton structure (simplified pixel positions)
      const skeletonPattern = [
        // Skull
        [7,2], [8,2], [9,2], [10,2],
        [6,3], [7,3], [8,3], [9,3], [10,3], [11,3],
        [6,4], [7,4], [10,4], [11,4], // Eyes
        [6,5], [7,5], [8,5], [9,5], [10,5], [11,5],
        [7,6], [8,6], [9,6], [10,6],
        // Spine
        [8,7], [9,7],
        [8,8], [9,8],
        [8,9], [9,9],
        // Ribs
        [7,8], [10,8],
        [6,9], [11,9],
        [7,10], [10,10],
        // Arms
        [5,9], [12,9],
        [4,10], [13,10],
        [3,11], [14,11],
        // Pelvis
        [7,11], [8,11], [9,11], [10,11],
        // Legs
        [7,12], [10,12],
        [7,13], [10,13],
        [7,14], [10,14],
        [6,15], [11,15]
      ];

      // Draw skeleton with glitch effect
      skeletonPattern.forEach(([x, y]) => {
        const colors = ['#00ff00', '#ff00ff', '#00ffff', '#ffff00'];
        const color = colors[Math.floor(Math.random() * colors.length)];
        
        // Apply glitch offset
        const glitchX = Math.random() < glitchIntensity ? (Math.random() - 0.5) * pixelSize * 2 : 0;
        const glitchY = Math.random() < glitchIntensity ? (Math.random() - 0.5) * pixelSize * 2 : 0;
        
        ctx.fillStyle = color;
        ctx.fillRect(
          x * pixelSize + glitchX, 
          y * pixelSize + glitchY, 
          pixelSize, 
          pixelSize
        );

        // Random glitch duplicates
        if (Math.random() < glitchIntensity * 0.5) {
          ctx.fillStyle = color;
          ctx.globalAlpha = 0.5;
          ctx.fillRect(
            x * pixelSize + (Math.random() - 0.5) * pixelSize * 3,
            y * pixelSize + (Math.random() - 0.5) * pixelSize * 3,
            pixelSize,
            pixelSize
          );
          ctx.globalAlpha = 1;
        }
      });

      // Add scan lines
      ctx.fillStyle = 'rgba(0, 255, 0, 0.1)';
      for (let i = 0; i < height; i += 4) {
        ctx.fillRect(0, i, width, 2);
      }
    }

    function createArtCard(tokenId) {
      // Re-seed PRNG for this token
      seed = getSeed(tokenId);
      Math.random = seededRandom; 
      
      const card = document.createElement('div');
      card.className = 'art-card';
      
      // Canvas size: 160x160 (20x20 grid of 8px pixels)
      const canvas = document.createElement('canvas');
      canvas.className = 'skeleton-art';
      canvas.width = 160; 
      canvas.height = 160;

      const artCanvas = document.createElement('div');
      artCanvas.className = 'art-canvas';
      // ‡∏õ‡∏£‡∏±‡∏ö‡∏Ç‡∏ô‡∏≤‡∏î art-canvas ‡πÉ‡∏´‡πâ‡πÄ‡∏Ç‡πâ‡∏≤‡∏Å‡∏±‡∏ö grid-card
      artCanvas.style.height = 'auto'; 
      artCanvas.style.aspectRatio = '1 / 1';
      artCanvas.appendChild(canvas);

      const glitchOverlay = document.createElement('div');
      glitchOverlay.className = 'glitch-overlay';
      artCanvas.appendChild(glitchOverlay);

      const artId = document.createElement('div');
      artId.className = 'art-id';
      artId.textContent = `#${String(tokenId).padStart(4, '0')}`;

      // Glitch Intensity (‡πÄ‡∏õ‡πá‡∏ô Trait)
      const glitchIntensity = 0.2 + Math.random() * 0.4; // 0.2 to 0.6

      // Rarity (‡πÄ‡∏õ‡πá‡∏ô Trait)
      const rarityIndex = Math.floor(Math.random() * rarityTypes.length);
      const rarity = document.createElement('div');
      rarity.className = 'art-rarity';
      rarity.textContent = rarityTypes[rarityIndex];

      card.appendChild(artCanvas);
      card.appendChild(artId);
      card.appendChild(rarity);

      // Generate art
      generateSkeletonPixelArt(canvas, glitchIntensity);

      // Add hover effect to re-generate (if allowed)
      // Note: OpenSea ‡∏≠‡∏≤‡∏à‡∏à‡∏∞‡πÑ‡∏°‡πà‡∏≠‡∏ô‡∏∏‡∏ç‡∏≤‡∏ï‡πÉ‡∏´‡πâ‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÅ‡∏õ‡∏•‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ô‡∏≥‡πÑ‡∏õ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•
      card.addEventListener('mouseenter', () => {
        // ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï Seed ‡∏Å‡πà‡∏≠‡∏ô‡∏™‡∏∏‡πà‡∏°‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏†‡∏≤‡∏û‡∏¢‡∏±‡∏á‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ö‡∏ö‡πÄ‡∏î‡∏¥‡∏°‡∏´‡∏≤‡∏Å‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÇ‡∏Æ‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏´‡∏•‡∏≤‡∏¢‡∏Ñ‡∏£‡∏±‡πâ‡∏á 
        // ‡∏´‡∏£‡∏∑‡∏≠‡∏à‡∏∞‡∏•‡∏ö‡∏™‡πà‡∏ß‡∏ô‡∏ô‡∏µ‡πâ‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ‡πÄ‡∏•‡∏¢‡∏Å‡πá‡πÑ‡∏î‡πâ
        seed = getSeed(tokenId);
        Math.random = seededRandom; 
        generateSkeletonPixelArt(canvas, glitchIntensity);
      });
      
      card.addEventListener('mouseleave', () => {
        // ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï Seed ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏†‡∏≤‡∏û‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡πÇ‡∏Æ‡πÄ‡∏ß‡∏≠‡∏£‡πå (‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏†‡∏≤‡∏û‡∏Ñ‡∏á‡∏ó‡∏µ‡πà)
        seed = getSeed(tokenId);
        Math.random = seededRandom; 
        generateSkeletonPixelArt(canvas, glitchIntensity);
      });


      return card;
    }

    function generateGallery(count = 1) {
      const grid = document.getElementById('artGrid');
      grid.innerHTML = '';
      
      // ‡∏î‡∏∂‡∏á ID ‡∏Ç‡∏≠‡∏á‡πÇ‡∏ó‡πÄ‡∏Ñ‡∏ô‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
      const tokenId = getTokenId(); 
      
      // ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡πå‡∏î‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö NFT ‡∏ä‡∏¥‡πâ‡∏ô‡∏ô‡∏µ‡πâ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô
      grid.appendChild(createArtCard(tokenId));
      
      // ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤ Math.random() ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏î‡∏¥‡∏° (‡πÑ‡∏°‡πà‡∏à‡∏≥‡πÄ‡∏õ‡πá‡∏ô‡∏ñ‡πâ‡∏≤‡πÇ‡∏Ñ‡πâ‡∏î‡∏ô‡∏µ‡πâ‡∏£‡∏±‡∏ô‡πÅ‡∏Ñ‡πà‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß)
      Math.random = _originalMathRandom; 
    }

    // ‡∏•‡∏ö event listener ‡∏Ç‡∏≠‡∏á‡∏õ‡∏∏‡πà‡∏° generateBtn ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏Å‡∏î‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏á‡∏≤‡∏ô‡∏®‡∏¥‡∏•‡∏õ‡∏∞‡πÉ‡∏ô NFT ‡πÄ‡∏î‡∏µ‡πà‡∏¢‡∏ß
    // document.getElementById('generateBtn').addEventListener('click', () => {
    //   generateGallery(1); // ‡∏´‡∏≤‡∏Å‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î ‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏Ñ‡πà 1 ‡∏ä‡∏¥‡πâ‡∏ô
    // });
    
    // ‡∏•‡∏ö‡∏™‡πà‡∏ß‡∏ô‡∏Ç‡∏≠‡∏á SDK ‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏à‡∏≥‡πÄ‡∏õ‡πá‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏ö‡∏ô OpenSea
    
